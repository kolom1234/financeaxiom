{
  "name": "@ofp/worker",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "wrangler dev",
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\" --config ../../eslint.config.mjs",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test:unit": "vitest run --config vitest.config.ts test/unit",
