    "test:compliance": "vitest run --config vitest.config.ts test/compliance"
  },
  "dependencies": {
    "@ofp/policy": "0.1.0",
    "@ofp/shared": "0.1.0",
    "jose": "^5.9.6",
    "pg": "^8.16.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260216.0",
    "wrangler": "^4.3.0"
  }
}
